<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../layout/head') %>

</head>
<body>
  <main>
      <aside class="dropdown">
        <div class="container">
          <div class="bar1"></div>
          <div class="bar2"></div>
          <div class="bar3"></div>
        </div>
          <nav class="navBar">
              <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/searches/new">New Search </a></li>
              </ul>
          </nav>
      </aside>

  <section>
    <h1>Hi, Cait, Vij, Ally! Lets read some books together</h1>
    <ul>
    <% results.forEach(obj => { %>
        <li id="title"><%= obj.title %></li>
        <li>By <%= obj.authors %></li>
        <li><img src="<%= obj.image_url %>" alt="<%= obj.title %>"></li>
        <li><%= obj.bookshelf %></li>
        <a href="/books/<%= obj.id %>"> View Details </a>
        
        <% }) %>
    </ul>
    </section>

  </main>


</body>
</html>